
let randomNumber = Math.floor(Math.random() * 8);
let eightBall = '';
switch (randomNumber) {
  case 0:
  eightBall = 'Very lucky!'
  break;
  case 1:
   eightBall = 'Is is certain'
  break;
  case 2:
  eightBall = 'It is decidedly so'
  break;
  case 3:
  eightBall = 'Reply hazy try again'
  break;
  case 4:
  eightBall = 'Cannout predict now'
  break;
  case 5:
  eightBall = 'I\'m good at guessing'
  break;
  case 6:
  eightBall = 'My sources say no'
  break;
  case 7:
  eightBall = 'Signs point to yes'
  break;
  case 8:
  eightBall = 'Outlook is looking good!'
  break;
}
console.log(eightBall);
